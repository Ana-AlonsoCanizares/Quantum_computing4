      "source": [
        "# Crear un modelo SVM: linear\n",
        "svm_model_3 = SVC(kernel='rbf', C = 1000, gamma = 30)\n",
        "\n",
        "# Entrenar el modelo en el conjunto de entrenamiento\n",
        "svm_model_3.fit(data_test[['Pregnancies',\t'Glucose',\t'BloodPressure',\t'SkinThickness',\t'Insulin',\t'BMI', 'DiabetesPedigreeFunction',\t'Age']])\n",
        "\n",
        "# Calcular la precisión en el conjunto de prueba\n",
        "accuracy = svm_model_3.score(data_test['Pregnancies',\t'Glucose',\t'BloodPressure',\t'SkinThickness',\t'Insulin',\t'BMI', 'DiabetesPedigreeFunction',\t'Age'], data_test['Outcome'])\n",
        "print(f'Precisión en el conjunto de prueba: {accuracy * 100:.2f}%')\n",
